{
  "name": "ai-tutor-api",
  "version": "1.0.0",
  "description": "AI Tutor Backend API with Express, TypeORM, PostgreSQL, Redis, and RabbitMQ",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate src/database/migrations/Migration -d src/config/datasource.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/config/datasource.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/config/datasource.ts",
    "migration:show": "npm run typeorm -- migration:show -d src/config/datasource.ts",
    "seed": "ts-node src/database/seeds/index.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "amqplib": "^0.10.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.1",
    "firebase-admin": "^12.7.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.13.0",
    "razorpay": "^2.9.4",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0",
    "winston": "^3.15.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.7.5",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "ai-tutor",
    "education",
    "express",
    "typescript",
    "typeorm",
    "postgresql",
    "redis",
    "rabbitmq",
    "claude-ai",
    "e2e-encryption"
  ],
  "author": "KA Software",
  "license": "MIT"
}
